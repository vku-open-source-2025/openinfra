PROJECT_NAME="Infrastructure Management System"
MONGODB_URL=mongodb://mongo:27017
DATABASE_NAME=gis_db
ADMIN_JWT_SECRET=change-me
ADMIN_JWT_ALGORITHM=HS256
ADMIN_JWT_EXPIRE_MINUTES=1440
ADMIN_DEFAULT_USERNAME=admin
ADMIN_DEFAULT_PASSWORD=admin123

# Cloudflare Turnstile (Captcha for public forms)
TURNSTILE_SECRET_KEY=your-turnstile-secret-key-here
TURNSTILE_SITE_KEY=your-turnstile-site-key-here

# AI Agent - Google Gemini API
# Get your API key at: https://aistudio.google.com/app/apikey
GEMINI_API_KEY=
GEMINI_TEXT_MODEL=text-embedding-004
GEMINI_VISION_MODEL=gemini-2.5-flash

# Duplicate Detection Configuration
# Similarity threshold (0.0-1.0) - incidents above this score are considered duplicates
DUPLICATE_SIMILARITY_THRESHOLD=0.85
# Weight for text similarity vs image similarity (0.0-1.0)
DUPLICATE_TEXT_WEIGHT=0.6
DUPLICATE_IMAGE_WEIGHT=0.4
# Time window in hours to search for duplicates (default: 168 = 7 days)
DUPLICATE_TIME_WINDOW_HOURS=168
# Location radius in meters to search for duplicates (default: 50 meters)
DUPLICATE_LOCATION_RADIUS_METERS=50.0
# Maximum number of images per incident to analyze for similarity
GEMINI_MAX_IMAGES_PER_INCIDENT=5
